/*复选框checkbox*/
input[type=checkbox]{
    -webkit-appearance: none;
    background-color: transparent;
    outline: 0 !important;
    cursor: pointer;
    vertical-align: bottom;
    position: relative;
}
/*checkbox1--对号*/
input[type=checkbox].checkbox1{
    background: url(../images/img/checkbox.png) no-repeat center;
    background-position: 0 0;
}
input[type=checkbox].checkbox1:checked {
    background: url(../images/img/checkbox.png);
    background-position: 0 -14px;
}

/*checkbox2---圆点*/
input[type=checkbox].checkbox2{
    background: url(../images/img/radio2.png) no-repeat center;
    background-position: 0 -12px;
}
input[type=checkbox].checkbox2:checked {
    background: url(../images/img/radio2.png);
    background-position: 0 0;
}

/*radio*/
input[type=radio]{
    -webkit-appearance: none;
    cursor: pointer;
    vertical-align: bottom;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    position: relative;
}
/*raido1 对号--大图标*/
input[type=radio].radio1{
    background: url(../images/img/radio1.png) no-repeat center;
    background-position: 0 -21px;
}
input[type=radio].radio1:checked {
    background: url(../images/img/radio1.png)  no-repeat center;
    background-position: 0 0;
}
/*radio2  圆点--小图标*/
input[type=radio].radio2{
    background: url("../images/img/radio2.png") no-repeat center;
    background-size: 100%;
    background-position: 0 -12px;
}
input[type=radio].radio2:checked {
    background: url("../images/img/radio2.png") center no-repeat;
    background-size: 100%;
    background-position: 0 0;
}

/*红底白字关闭按钮图标*/
