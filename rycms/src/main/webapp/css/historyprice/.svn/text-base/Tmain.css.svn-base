* {
	margin: 0;
	padding: 0;
	font-family: Arial, "Arial Black", "微软雅黑", "宋体";
}
html{overflow-y:scroll; overflow-x:scroll}
ul { list-style: none; }
a {
	text-decoration: none;
	color: #000;
}
form{overflow:hidden;}
select { width: 200px }
table { border-collapse: collapse; }
table.cont {
	font-size: 13px;
	letter-spacing: 1px;
	border: 1px solid #E1E5EE;
}
table.cont tr td {
	height: 20px;
	padding: 5px 0;
	border-bottom: 1px solid #E1E5EE;
}
table.cont tr th {
	height: 20px;
	padding: 5px 0;
	border-bottom: 1px solid #E1E5EE;
	background-color: #E7F1FE;
}
table.cont input.text {
	height: 20px;
	width: 150px;
}
table.cont select option {
	height: 20px;
	width: 120px;
}
table.cont .btn { width: 100px; }
body {
	font-size: 12px;
	color: #000;
	background-color: #EEF2FB;
}
.title {
	margin: 0 5px;
	width: 100px;
	font-size: 16px;
	line-height: 45px;
	font-weight: bold;
	background: url(../images/top_bt.jpg) no-repeat;
	display: block;
	text-indent: 10px;
	padding-top: 2px;
	float: left;
}
.left_txt {
	line-height: 25px;
	color: #666666;
}
.title-1 {
	font-size: 18px;
	font-weight: bold;
	line-height: 30px;
	margin-left: 10%;
	padding-top: 5px;
	text-align: center;
	text-indent: 15px;
	width: 80%;
}
title-1 h1 { width: 100%; }
.title-2 {
	font-weight: bold;
	line-height: 30px;
	text-indent: 15px;
	margin-top: 20px;
}
.title-2 input {
	height: 30px;
	width: 90%;
	text-align: left;
}
.title-3 {
	width: 100%;
	margin: 30px 0;
	text-align: center;
}
.title-3 tr td { width: 33%; }
.title-3 tr input {
	width: 100px;
	line-height: 30px;
	text-align: center;
}
.tcbg {
	width: 100%;
	height: 100%;
	background-color: rgba(0,0,0,0.5);
	display: none;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 2;
}
.tc {
	width: 1000px;
	height:600px;
	position: fixed;
	top: 5%;
	left: 5%;
	background: #ffffff;
	display: none;
	z-index: 100;
	overflow:auto;
	overflow-x:scroll
}
table.title-4 {
	width: 90%;
	margin: 0 5%;
	font-size: 13px;
	letter-spacing: 1px;
	border: 1px solid #E1E5EE;
	overflow:hidden;
}
table.title-4 h2 { margin: 20px 0; }
table.title-4 h3 { margin: 5px 0; }
table.tr_color tr.d:hover { background-color: #E4F0DF; }
table.tr_color tr.d input {
	padding: 0 10px;
	margin-left: 5px;
}
table.title-4 tr td {
	height: 25px;
	padding: 5px 0;
}
table.title-4 tr th {
	height: 25px;
	padding: 5px 0;/*border: 1px solid #E1E5EE;
	background-color: #E7F1FE;*/
}
table.title-4 input.text {
	height: 20px;
	width: 150px;
}
table.title-4 select option {
	height: 20px;
	width: 120px;
}
table.title-4 form {
	height: auto;
	overflow-x: hidden;
	padding: 10px 0;
}
table.title-4 tr, table.title-4 th, table.title-4 tr td { border: none }
table.title-4 .btn { width: 150px; }
table.title-4 input.tcinput {
	width: 80%;
	height: 25px;
	margin-left: 5px;
}
.title-4 img {
	width: 100%;
	max-height: 180px;
}
.cont1 { border: 1px solid #E1E5EE; }
.title-3 {
	display: block;
	float: left;
	font-weight: bold;
	line-height: 26px;
	margin: 0 40%;
	padding-top: 5px;
	text-indent: 15px;
	width: 20%;
}
.title-5 {
	width: 90%;
	margin-left: 5%;
	margin-bottom: 30px;
}
.title-5 tr {
	vertical-align: top;
	font-size: 14px;
	line-height: 18px;
}
.title-5 input {
	line-height: 30px;
	width: 100px;
	text-indent: 6px;
}
/*foot*/
.foot {
	position: absolute;
	top: 90%;
}
