@charset "utf-8";
.main_visual {
	width:100%;
	height: 150px;
	border-top: 1px solid #d7d7d7;
	overflow: hidden;
	position: relative;
}
.main_image {
	width:100%;
	height: 150px;
	overflow: hidden;
	position: relative;
}
.main_image ul {
	width: 100%;
	height: 150px;
	overflow: hidden;
	position: absolute;
	top: 0;
	left: 0
}
.main_image li {
	float: left;
	width: 100%;
	height: 150px;
}
.main_image li span {
	display: block;
	width: 100%;
	height: 150px
}
.main_image li a {
	display: block;
	width: 100%;
	height: 150px
}
.banner{
	width:100%;
	height:150px;;
}
div.flicking_con {
	position: absolute;
	top: 120px;
	left: 0;
	z-index: 999;
	width: 40%;
	height: 21px;
	margin: 0 0 0 45%;
}
div.flicking_con a {
	float: left;
	width: 21px;
	height: 21px;
	margin: 0;
	padding: 0;
	background: url('../images/btn_main_img.png') 0 0 no-repeat;
	display: block;
	text-indent: -1000px
}
div.flicking_con a.on {
	background-position: 0 -21px
}