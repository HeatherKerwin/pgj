package com.ry.core.form;

import java.util.List;

import com.ry.core.entity.Systeminfo;

/**
 * 消息条件[订单和优惠]
 * @author WKX
 */
public class SysteminfoForm extends Systeminfo{
	
	private static final long serialVersionUID = 1L;
	
	private List<Type> types;//查询多个类型
	
	Systeminfo systeminfo = new Systeminfo();

	public Integer getMemberId() {
		return systeminfo.getMemberId();
	}

	public void setMemberId(Integer memberId) {
		systeminfo.setMemberId(memberId);
	}

	public Type getType() {
		return systeminfo.getType();
	}

	public void setType(Type type) {
		systeminfo.setType(type);
	}

	public List<Type> getTypes() {
		return types;
	}

	public void setTypes(List<Type> types) {
		this.types = types;
	}
}