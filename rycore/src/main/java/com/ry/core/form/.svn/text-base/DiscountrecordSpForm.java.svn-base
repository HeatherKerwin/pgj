package com.ry.core.form;

import com.ry.core.entity.DiscountrecordSp;

public class DiscountrecordSpForm extends DiscountrecordSp {

	private static final long serialVersionUID = 1L;
	
	private Integer[] state;//可能同时查看多个状态
	
	private Integer comment;//评价标示（0已评价,1未评价）
	
	private Double minallmoney;//pc端搜索添加
	
	private Double maxallmoney;//pc端搜索添加

	public Double getMinallmoney() {
		return minallmoney;
	}

	public void setMinallmoney(Double minallmoney) {
		this.minallmoney = minallmoney;
	}

	public Double getMaxallmoney() {
		return maxallmoney;
	}

	public void setMaxallmoney(Double maxallmoney) {
		this.maxallmoney = maxallmoney;
	}

	public Integer[] getState() {
		return state;
	}

	public void setState(Integer[] state) {
		this.state = state;
	}

	public Integer getComment() {
		return comment;
	}

	public void setComment(Integer comment) {
		this.comment = comment;
	}
}