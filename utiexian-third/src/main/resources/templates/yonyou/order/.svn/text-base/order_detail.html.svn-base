<script>
/**
* 价格真实
*/
Handlebars.registerHelper('toPrice', function(price, options) {
    if(price != null && price != "" && price != "--"){
    	return (price*1).toFixed(2);
    }else{
    	return "--";
    }
});

/**
* 服务态度
*/
Handlebars.registerHelper('toService', function(service, options) {
    if(service != null && service != "" && service != "--"){
    	return (service*1).toFixed(2);
    }else{
    	return "--";
    }
});

/**
* 确认效率
*/
Handlebars.registerHelper('toSpeed', function(speed, options) {
    if(speed != null && speed != "" && speed != "--"){
    	return (speed*1).toFixed(2);
    }else{
    	return "--";
    }
});

