    		dataType:"json",
    		success:function(data){
				if(data.response == 'success'){
					$("#title").html("同意票据转让合同");
			        $("#maskWindow").fadeOut(500); /*内容区域*/
			        $("#contractWindow").fadeOut(500); /*选择机构*/
			        $("#maskFooter").fadeOut(500); /*底部按钮区域*/
			        $("#contractFooter").fadeOut(500); /*选择机构*/
					location.href = "${basePath}/yonyou/page/order/list";
				}else{
					alert(data.msg);
				}
    		}
		});
	};
	
