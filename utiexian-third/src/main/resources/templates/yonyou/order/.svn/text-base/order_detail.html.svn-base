                        <a href="javascript:void(0);" class="lineBtn" style="display: none" id="cancel">取消订单</a>
                        <a href="javascript:void(0);" class="lineBtn" style="display: none" id="pay">去支付</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<!--弹窗-->
<div class="maskWindow" id="maskWindow" style="display: none">
    <div class="maskBg"></div>
    <div class="maskWrap maskWrapSmall">
        <!--top-->
        <div class="maskTop">
            <!--弹窗名称-->
            <div id="title" class="maskTitle">开户合同</div>
            <!--关闭按钮-->
            <a class="maskClose">
                <img src="${basePath}/images/yonyou/common/closeIcon.png" alt="关闭">
            </a>
        </div>
        <!--top end-->

        <!--内容容器-->
        <div class="maskCon">

            <!--选择机构-->
            <div class="maskDiv" id="selectionWindow" style="display: none">
                <div class="flex-row pl105 mt40">
                    公司名称：<span id="confirm_company"></span>
                <input type="hidden" id="selectDistId" value=""/>
                </div>
                <div class="flex-row pl105">
                    票款金额：<span class="cd43c33" id="confirm_txje"></span>
                </div>
                <div class="flex-row pl105">
                    您是否选择该机构？
                </div>

            </div>
            <!--选择机构end-->

            <!-- 下载交易合同 -->
            <div class="maskDiv" id="sendEmailWindow" style="display: none">
                <ul class="maskForm">
                    <li>
                        <label for="email">发送合同至您的邮箱</label>
                        <input type="email" id="email" class="validate[required,custom[email]]" maxlength="30" placeholder="请输入经办人邮箱号" style="width: 300px">
                    </li>
                </ul>
            </div>
            <!-- 下载交易合同end -->

            <!--取消订单-->
