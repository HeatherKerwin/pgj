	/**
	* 票据合同
	*/
	function contract(){
		var distId = $("#distId").val();
		var orderType = 'DISCOUNTRECORD';
		
		$.ajax({
    		url:'${basePath}/yonyou/cib/contract/info/by/id',
    		type:"POST",
    		data:{distId:distId,orderType:orderType},
    		dataType:"json",
    		success:function(data){
				if(data.response == 'success'){
					$("#bnsName").html(data.data.bnsName);
					$("#bnsBizLicenceLegalName").html(data.data.bnsBizLicenceLegalName);
					$("#bnsBizLicenceAddr").html(data.data.bnsBizLicenceAddr);
					$("#orgName").html(data.data.orgName);
					$("#orgBizLicenceLegalName").html(data.data.orgBizLicenceLegalName);
					$("#orgBizLicenceAddr").html(data.data.orgBizLicenceAddr);
					$("#no").html(data.data.no);
					$("#draftNo").html(data.data.draftNo);
				}else{
					alert(data.msg);
				}
    		}
		});
	};
	
