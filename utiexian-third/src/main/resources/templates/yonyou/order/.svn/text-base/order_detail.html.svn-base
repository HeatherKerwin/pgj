                            </ul>
                        </div>
                    </div>

                    <!--银电-->
                    <div class="detailBasic yinzhi" style="display: none">
                        <!--票据市场：回头票标志-->
                        <div class="ticketReturn" style="display: none"><img src="${basePath}/images/yonyou/common/returnTicket.png" alt="回头票"></div>
                        <!--票据各属性名-->
                        <ul class="showTitle">
                            <li class="w220">银票订单号</li>
                            <li class="w150">票面金额</li>
                            <li class="w140">背书</li>
                            <li class="w140">保证金</li>
                            <li class="w308">票据特征</li>
                        </ul>
                        <!--票据各属性值-->
                        <div class="showMain">
                            <div class="showLeft RrightLine">
                                <ul class="showLeftTop RrightLine">
                                    <li class="w220 RrightLine"><span class="ordernumber"></span><a class="download contractBtn" title="下载合同"></a></li>
                                    <li class="w150 RrightLine ce84c3d"><span class="allmoney"></span>万元</li>
                                    <li class="w140 RrightLine endorse"></li>
                                    <li class="w140"><span class="deposit"></span>元</li>
