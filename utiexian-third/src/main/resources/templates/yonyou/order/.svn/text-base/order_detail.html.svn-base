                <ul class="maskForm">
                    <li>
                        <label>请选择取消理由：</label>
                        <select id="reason">
                            <option value="0">票面信息输入有误</option>
                            <option value="1">只为熟悉流程和询问价格</option>
                            <option value="2">价格不合适</option>
                            <option value="3">已提前出票</option>
                            <option value="4">其它</option>
                        </select>
                    </li>
                    <li style="display: none">
                        <textarea id="reasonOther" maxlength="100" placeholder="最多100字的理由。" class="validate[required]"></textarea>
                    </li>
                </ul>
            </div>
            <!--取消订单end-->

            <!--评价-->
            <div class="maskDiv" id="evaluateWindow" style="display: none">
                <!--票据信息-->
