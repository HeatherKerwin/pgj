#include("./common/header.html", {title:"更改密码"})
#include("./common/right.html")
#include("./common/my_menu.html")
<div class="container">
	<div class="row clearfix">
		<div class="col-md-12 column calendar-viewer">
			<form id="password-form" class="form-horizontal" role="form">
				<div class="form-group">
					 <label for="curpwd" class="col-sm-2 control-label">当前密码</label>
					<div class="col-sm-5">
						<input type="password" class="form-control input-sm" id="curpwd" maxlength="11" autocomplete="off" required="required"/>
					</div>
				</div>
				<div class="form-group">
					 <label for="newpwd" class="col-sm-2 control-label">新密码</label>
					<div class="col-sm-5">
						<input type="password" class="form-control input-sm" id="newpwd" maxlength="18" autocomplete="off" required="required"/>
					</div>
				</div>
				<div class="form-group">
					 <label for="renewpwd" class="col-sm-2 control-label">确认新密码</label>
					<div class="col-sm-5">
						<input type="password" class="form-control input-sm" id="renewpwd" maxlength="18" autocomplete="off" required="required"/>
					</div>
				</div>
				<div class="form-group">
					<div class="col-sm-offset-2 col-sm-5">
						 <button type="button" class="btn btn-default btn-danger btn-block submit">确认修改</button>
					</div>
				</div>
			</form>
		</div>
	</div>
</div>
#include("./common/footer.html") 
</body>
</html>