@CHARSET "UTF-8";

.mt-20{
	margin-top: -20px
}
.mb130{
	margin-bottom: 130px;
}

/* 首页轮播 */
.arrow-left{
	background: url("https://img.utiexian.com/bbs/bbs/arrow-left.png") center no-repeat;
	background-size: 100% auto; 
}
.arrow-right{
	background: url("https://img.utiexian.com/bbs/bbs/arrow-right.png") center no-repeat;
	background-size: 100% auto; 
}

/* 登录图像 */
.account-avatar {
}
.account-avatar, .account-details {
    display: table-cell;
    vertical-align: top;
}
/* 账户头像 */
.account-avatar img {
    width: 55px;
    height: 55px;
}
/* 达人头像 */
.account-avatar img.DR-img {
    width: 40px;
    height: 40px;
}
.account-avatar img.head-img {
    width: 46px;
    height: 46px;
}
.thumbnail {
    display: block;
    padding: 4px;
    line-height: 1;
    -webkit-border-radius: 55px;
    -moz-border-radius: 55px;
    border-radius: 55px;
    -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075);
    -moz-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075);
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.075);
}
.account-avatar, .account-details {
    display: table-cell;
    vertical-align: top;
}
.account-details .account-name {
    font-weight: 600;
}
.account-details span {
	font-size: 15px;
/* 	line-height:25px; */
    display: block;
}
.account-role a , .account-actions b{
    color: #e84c3d;
}

/* 已签到 */
.btn-sign {
	color: #fff;
	background-color: #a5a5a5
}

/* 帖子点赞小图标 */
a.span_link{
	padding: 5px;
	color: #a5a5a5;           
/* 	font-size: 14px; */
	line-height: 20px;
	display: inline-block;
	}
.eyeIcon , .commentIcon , .thumbsIcon , .heartIcon{
	display: inline-block;
	width: 13px;
	height: 13px;
	margin-right: 5px;
}
.eyeIcon{
	background: url("https://img.utiexian.com/bbs/bbs/eye.png");
	background-size: 100% 100%; 
}
.commentIcon{
	background: url("https://img.utiexian.com/bbs/bbs/comment.png");
	background-size: 100% 100%; 
}
.thumbsIcon{
	background: url("https://img.utiexian.com/bbs/bbs/thumbs.png");
	background-size: 100% 100%; 
}
.heartIcon{
	background: url("https://img.utiexian.com/bbs/bbs/heart.png");
	background-size: 100% 100%; 
}


/* 引用文本 */
.blockquote {
    padding: 3px 20px;
    margin: 0 ;
    font-size: 17.5px;
    border-left: 3px solid #d43c33;
}
/* 文本超出隐藏省略号 */
.table-hidden{
	overflow:hidden;
	white-space:nowrap;
	text-overflow:ellipsis;
}

.widget-left {
	height: 80px;
	padding-top: 15px;
	text-align: center;
	border-top-left-radius: 4px;
	border-bottom-left-radius: 4px;
}

.widget-right {
	text-align: left;
	line-height: 1.6em;
	margin: 0px;
	padding: 20px;
	height: 80px;
	color: #999;
	font-weight: 300;
	border-top-right-radius: 4px;
	border-bottom-right-radius: 4px;
}

/* 帖子内容 */
/* 标题 */
.comment a.post {
	color: #2d2d2d;
	font-size: 16px;
}
.box-tab .tab-content1 {
	padding: 15px 15px 0;
	background-color: #fff
}
/* 背景蓝色 */
.list-blue{
	font-size:16px;
	color: #fff;
	background-color: #3366cc;
	border: 1px solid #3366cc; 
}
/* 字体颜色 */
a.text-blue {
	color: #5577e7
}

a.text-blue:hover {
	color: #778ffd
}

/* 左侧 */

@media (min-width: 768px) {
	.offer {
	    border-radius: 0;
	    box-shadow: none;
	    border-width: 0;
	    position: relative;
	    top: 0;
	    left: 0;
	    margin-bottom: 20px;
    }
}
@media (min-width: 992px) {
	.offer {
	    width: 705px;
	    border-radius: 0;
	    box-shadow: none;
	    border-width: 0;
	    position: relative;
	    top: 0;
	    left: 0;
	    margin-bottom: 20px;
    }
}
@media (min-width: 1200px){
	.offer {
	    width: 190px;
	    height:370px;
	    position: fixed;
	    top: 26%;
	    left: 0;
	    z-index:100;
    }
}
.offer_close{
	position: relative;
	top:22px;
	left:168px;
	z-index: 10;
	width:20px;
	height:20px;
	cursor: pointer;
	background: url("https://img.utiexian.com/bbs/bbs/close2.png");
	background-size:100% 100%;
}
.offer_close:hover{
	position: relative;
	top:22px;
	left:168px;
	z-index: 10;
	width:20px;
	height:20px;
	cursor: pointer;
	background: url("https://img.utiexian.com/bbs/bbs/close1.png");
	background-size:100% 100%;
}
.offer-con2{
	display:none;
	width: 36px;
	background: #778fdd;
	color: #fff;
	font-size:18px;
	line-height: 26px;
	padding: 8px;
	position: fixed;
	top: 51%;
	left: 0;
	z-index:100;
	cursor: pointer;
}

/*抽奖*/
@media (min-width: 992px) {
	.choujiang-con {
		width: 500px;margin: 0 auto
	}
}

/*右侧浮动*/
@media (min-width: 768px) {
	.rightDIV {
		display: none;
	}
}
@media (min-width: 992px) {}
@media screen and (min-width:768px) {
	.rightDIV{
		display:block;
		width: 180px;
		height:300px;
		position:fixed;
		top: 420px;
		right: 0;
		z-index: 10000
	}
	#moquu_top,#moquu_wxin,#moquu_wshare,#moquu_wmaps {
	    width:50px;
	    height:50px;
	    left: 130px;
	    position:absolute;
	    cursor:pointer;
	    _position:absolute;
	    _bottom:auto;
	    z-index: 100003;
	    _top:expression(eval(document.documentElement.scrollTop+document.documentElement.clientHeight-this.offsetHeight-(parseInt(this.currentStyle.marginTop,10)||0)-(parseInt(this.currentStyle.marginBottom,10)||0)))
	}
	
	#moquu_wxin {
	    top:93px;
	    _margin-top:93px
	}
	#moquu_wxin a {
	    background: url('https://img.utiexian.com/bbs/bbs/right1.png');
	    right:0;
	    float:left;
	    width:50px;
	    height:50px;
	    text-indent:-9999px
	}
	#moquu_wxin a:hover {
	    background: url('https://img.utiexian.com/bbs/bbs/right11.png') -50px -0px
	}
	#moquu_wshare {
	    top:145px;
	    _margin-top:145px
	}
	#moquu_wshare a {
	    background: url('https://img.utiexian.com/bbs/bbs/right2.png');
	    width:50px;
	    height:50px;
	    right:0;
	    float:left;
	    text-indent:-9999px
	}
	#moquu_wshare a:hover {
	    background: url('https://img.utiexian.com/bbs/bbs/right22.png');
	}
	#moquu_wmaps {
	    top:197px;
	    _margin-top:197px
	}
	#moquu_wmaps a {
	    background: url('https://img.utiexian.com/bbs/bbs/right3.png');
	    width:50px;
	    height:50px;
	    right:0;
	    float:left;
	    text-indent:-9999px
	}
	#moquu_wmaps a:hover {
	    background: url('https://img.utiexian.com/bbs/bbs/right33.png');
	}
	#moquu_top {
	    top:249px;
	    _margin-top:249px;
	    background: url('https://img.utiexian.com/bbs/bbs/right4.png');
	    width:50px;
	    height:50px
	}
	#moquu_top:hover {
	    background: url('https://img.utiexian.com/bbs/bbs/right44.png');
	}
	.moquu_wxin,.moquu_wshare {
	    position:relative;
	    z-index:2
	}
	.moquu_wxin a:hover .moquu_wxinh,.moquu_wshare a:hover .moquu_wshareh {
	    display:block
	}
	.moquu_wxin .moquu_wxinh {
	    position:absolute;
	    display:none;
	    left:-130px;
	    top:-93px;
	    width:130px;
	    height:141px;
	    background: url('https://img.utiexian.com/bbs/bbs/right111.png') no-repeat
	}
	.moquu_wshare .moquu_wshareh {
	    position:absolute;
	    display:none;
	    left:-130px;
	    top:-93px;
	    width:130px;
	    height:141px;
	    background: url('https://img.utiexian.com/bbs/bbs/right222.png') no-repeat
	}
}

