/* 快速登录 */
.login-tabs>li>a {
	margin: 0;
	border: 0;
	padding: 10px 15px 5px
}
.login-tabs>li>a:focus, .login-tabs>li>a:hover {
	color: #2d2d2d;
	border: 0;
}

.login-tabs>li.active>a, .login-tabs>li.active>a:focus, .login-tabs>li.active>a:hover
	{
	color: #e84c3d;
	border-bottom: 1px solid #e84c3d;
}