button:focus,
button:active:focus,
button.active:focus,
button.focus,
button:active.focus,
button.active.focus {
    outline: none;
}

.btn-module{
    color: #ffffff;
    background: #9314de !important;
}

.banone{
    border: 0 !important;
    box-shadow: none !important;
}
.bcnone{background: none !important}

.mt10{margin-top: 1rem}

.warp{
    width: 100%;
    height: 100%;
    padding: 0;
    margin: 0;
}
.activityBg{
    /*position: absolute;*/
    top: 0;
    height: 0%;
    width: 100%;
    height: auto;
    z-index: 1;
}

.activityCon{
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: auto;
    z-index: 5;
}
/* 转盘游戏 */
#zhuanpan {
    width: 90%;
    height: auto;
    margin: 1rem auto;
    position: relative;
}
@media (max-width: 768px){
    #zhuanpan{
    }
}
@media (min-width: 768px) {
    #zhuanpan{
    }
}
#img{
    width:90%;
    height:auto;
    margin:0 auto;
}
#tip {
    width: 30%;
    height: 48%;
    position: absolute;
    top: 26%;
    left: 35%;
    cursor: pointer;
}

.timesDiv{
    width: 100%;
    margin: 2rem auto;
}
/*我的奖品*/
.myGift{
    color: #fdf579;
    margin-left: 2rem;
    line-height: 3.6rem;
    text-decoration: underline;
}
/*抽奖按钮*/
.playBtn{
    width: 40%;
    height: 3.6rem;
    line-height: 3.6rem;
    background: url("../images/btnBg.png") no-repeat center;
    background-size: 100% 100%;
    border: none;
    color: #c032f4;
    margin-left: 2rem;
}
/*增加次数*/
.addTimes{
    color: #fdf579;
    background: none;
    border: none;
    line-height: 3.6rem;
    margin-left: 2rem;
    text-decoration: underline;
}

.ruleDiv{
    position: relative;
}
/* 获奖名单滚动 */
ul.gundong , ul.gundong li{
    border: none;
    background: none !important;
    color: #fdf579;
}
ul.gundong{
    width: 80%;
    position: absolute;
    top: 4rem;
    left: 10%;
}


/*header*/
header{
    position: relative;
    background: #ffffff;
    padding: 1rem;
    line-height: 3rem;
}
header img.logo{
    position: absolute;
    width: auto;
    height: 2rem;
    left: 1rem;
}
header h3{
    padding: 0;
    margin: 0 !important;
    text-align: center;
}
