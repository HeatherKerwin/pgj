@charset "utf-8";
/* CSS Document */
*{ margin:0px; padding:0px;}
a{ text-decoration: none;}
#header
{ text-align:center; width:50%; margin-top:30px; margin-bottom:20px;}

#contet
{
	width:80%; 
	height:auto;
	margin-top:50px auto 50px; 
	border:1px solid #666; 
	overflow: hidden; 
	margin-left:10px;}
.li{padding-bottom:10px; margin-left:50px; padding-top:50px;}
.shangchuan input[type="submit"]
{
 	width:100px; 
 	height:50px;
 	margin-left:40%; 
	text-align:center; 
 	font-size:20px;
 	color:#000;  
 	border-radius:9px; 
 	margin-top:30px; 
	margin-bottom:50px; 
	border:1px solid #999; 
 	font-family:Tahoma, Geneva, sans-serif;
}
.img{max-width:800px; padding-bottom:20px; padding-left:60px;" width:100%;}


