}
/*注册 享优惠 */
function reg2(tag){
	if($(tag).attr("disabled")){
		return ;
	}
	var phone = $("#phone3").val();
	var code = $("#code3").val();
	var hezuo = $.cookie('hezuo');
	var uuid = $.cookie("uuid");
	checkMobile(phone);
	if(code==null || code.trim()==""){
		alert("请输入手机验证码");
		return ;
	}
	$(tag).attr("disabled","disabled");
	$("#hezuo2").val(hezuo);
	$("#uuid2").val(uuid);
	$("#qudao2").val("PC");
	$.ajax({
		url:websitePath+"/tg/register_yanzheng.htm",
		type:"post",
		data:{"phone":phone,"code":code},
		dataType:"text",
		success:function(data){
			if(data=="success"){
				$("#form2").submit();
				save("PC","homepage3","");
			}else{
				alert(data);
				$(tag).removeAttr("disabled");
				return false;
			}
		}
	});
}
function showtime(t){
	$(".login-codeR").attr({"disabled":"disabled"});
	var phoneNo = document.getElementById("phone").value;	
	var flag = 0;
    if (checkPhone(phoneNo)) {
    	$.ajax({
    		async:false,
    		url:websitePath+"/tg/sendcode",
    		type:"post",
    		data:{"phone":phoneNo,"flag":"reg"},
    		dataType:"text",
    		success:function(data){
    			if(data=="success"){
    				flag = 1;
    			}else{
    				flag = 0;
    				alert(data);
    				$(".login-codeR").removeAttr("disabled");
    			}
    		}
    	});
    	if(flag == 1){
	    	for(i=1;i<=t;i++) {
    			window.setTimeout("update_p(" + i + ","+t+")", i * 1000);
    		}
		}
	}else {
		alert("请输入正确的手机号！");
		$(".login-codeR").removeAttr("disabled");
	}
}
function showtime1(t){
	$(".login-codeR").attr({"disabled":"disabled"});
	var phoneNo = document.getElementById("phone2").value;	
	var flag = 0;
    if (checkPhone(phoneNo)) {
    	$.ajax({
    		async:false,
    		url:websitePath+"/tg/sendcode",
    		type:"post",
    		data:{"phone":phoneNo,"flag":"reg"},
    		dataType:"text",
    		success:function(data){
    			if(data=="success"){
    				flag = 1;
    			}else{
    				flag = 0;
    				alert(data);
    				$(".login-codeR").removeAttr("disabled");
    			}
    		}
    	});
    	if(flag == 1){
	    	for(i=1;i<=t;i++) {
    			window.setTimeout("update_p1(" + i + ","+t+")", i * 1000);
    		}
		}
	}else {
		alert("请输入正确的手机号！");
		$(".login-codeR").removeAttr("disabled");
	}
}
function showtime2(t){
	$(".int-codeBtn").attr({"disabled":"disabled"});
	var phoneNo = document.getElementById("phone3").value;	
	var flag = 0;
    if (checkPhone(phoneNo)) {
    	$.ajax({
    		async:false,
    		url:websitePath+"/tg/sendcode",
    		type:"post",
    		data:{"phone":phoneNo,"flag":"reg"},
    		dataType:"text",
    		success:function(data){
    			if(data=="success"){
    				flag = 1;
    			}else{
    				flag = 0;
    				alert(data);
    				$(".int-codeBtn").removeAttr("disabled");
    			}
    		}
    	});
    	if(flag == 1){
	    	for(i=1;i<=t;i++) {
    			window.setTimeout("update_p2(" + i + ","+t+")", i * 1000);
    		}
		}
	} else {
		alert("请输入正确的手机号！");
		$(".int-codeBtn").removeAttr("disabled");
	}
}
function update_p(num,t) {	
    if(num == t) {
        document.getElementById("phone_btn1").value =" 重新发送 ";
        document.getElementById("phone_btn1").disabled="";
    }else {
        printnr = t-num;
		document.getElementById("phone_btn1").value =printnr +"s重新发送";
    }
}
function update_p1(num,t) {	
    if(num == t) {
        document.getElementById("phone_btn2").value =" 重新发送 ";
        document.getElementById("phone_btn2").disabled="";
    }else {
        printnr = t-num;
		document.getElementById("phone_btn2").value =printnr +"s重新发送";
    }
}
function update_p2(num,t) {	
    if(num == t) {
        document.getElementById("phone_btn3").value =" 重新发送 ";
        document.getElementById("phone_btn3").disabled="";
    }else {
        printnr = t-num;
		document.getElementById("phone_btn3").value =printnr +"s重新发送";
    }
}
