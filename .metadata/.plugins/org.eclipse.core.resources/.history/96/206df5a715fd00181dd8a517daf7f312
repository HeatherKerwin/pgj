/*验证*/
function phone(){
	var x=document.getElementById("phone").value;
	document.getElementById("phone").value=x.toUpperCase();
	var y=document.getElementById("phone1").value;
	document.getElementById("phone1").value=y.toUpperCase();
	var z=document.getElementById("phone2").value;
	document.getElementById("phone2").value=z.toUpperCase();
	var h=document.getElementById("phone3").value;
	document.getElementById("phone3").value=h.toUpperCase();
}
	
function checkMobile(str) {
	var p = validateMobile(str);
	//如果手机号码不能通过验证
	if(str == null || str == ""){
		alert("手机号码不能为空");
		return false;
	}else if($.trim(str).length != 11){
		alert("请输入11位有效的手机号码！");
		return false;
	}else if(!p){
		alert("请输入正确的手机号！");
	    return false;
	} else {
		return true;
	}
}

function checkPhone(str) {
	var p = validateMobile(str);
	//如果手机号码不能通过验证
	if(!p){
	    return false;
	} else {
		return true;
	}
}

function reg(tag){
	if($(tag).attr("disabled")){
		return false;
	}
	var chk = $("#check").attr("checked");
	if(chk){
		var phone = $("#phone").val();
		var code = $("#code").val();
		var hezuo = $.cookie('hezuo');
		var uuid = $.cookie("uuid");
		checkMobile(phone);
		if(code==null || code.trim()==""){
			alert("请输入手机验证码");
			return false;
		}
		$(tag).attr("disabled","disabled");
		$("#hezuo").val(hezuo);
		$("#uuid").val(uuid);
		$("#qudao").val("PC");
		$.ajax({
			url:websitePath+"/tg/register_yanzheng.htm",
			type:"post",
			data:{"phone":phone,"code":code},
			dataType:"text",
			success:function(data){
				if(data=="success"){
					$("#form").submit();
					save("PC","homepage1","");
				}else{
					alert(data);
					$(tag).removeAttr("disabled");
					return false;
				}
			}
		});
	}else{
	    alert("协议还有什么不满意的么");
	    return false;
	}
}

function reg1(tag){
	if($(tag).attr("disabled")){
		return false;
	}
	var chk = $("#check2").attr("checked");
	if(chk){
		var phone = $("#phone2").val();
		var code = $("#code2").val();
		var hezuo = $.cookie('hezuo');
		var uuid = $.cookie("uuid");
		checkMobile(phone);
		if(code==null || code.trim()==""){
			alert("请输入手机验证码");
			return false;
		}
		$(tag).attr("disabled","disabled");
		$("#hezuo1").val(hezuo);
		$("#uuid1").val(uuid);
		$("#qudao1").val("PC");
		$.ajax({
			url:websitePath+"/tg/register_yanzheng.htm",
			type:"post",
			data:{"phone":phone,"code":code},
			dataType:"text",
			success:function(data){
				if(data=="success"){
					$("#form1").submit();
					save("PC","homepage1","");
				}else{
					alert(data);
					$(tag).removeAttr("disabled");
					return false;
				}
			}
		});
	}else{
