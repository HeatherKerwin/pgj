<!-- 手机号隐藏中间四位 -->
[#function hideMobile mob maxLength=10]
    [#assign mob=mob?trim]
	[#if (mob?length > maxLength)]
		[#return mob?substring(0,3) + "****" + mob?substring(7,11)]
	[#else]
		[#return mob]
	[/#if]
[/#function]

<!-- 手机号隐藏中间四位 -->
[#function getBankImg bank]
	[#if "ICBC"==bank]
		[#return "gongshang.png"]
	[#elseif "ABC"==bank]
		[#return "nongye.png"]
	[#elseif "CCB"==bank]
		[#return "jianshe.png"]
	[#elseif "BOC"==bank]
		[#return "zhonghang.png"]
	[#elseif "BCOM"==bank]
		[#return "jiaotong.png"]
	[#elseif "CIB"==bank]
		[#return "xingye.png"]
	[#elseif "CITIC"==bank]
		[#return "zhongxin.png"]
	[#elseif "CEB"==bank]
		[#return "guangda.png"]
	[#elseif "PAB"==bank]
		[#return "pingan.png"]
	[#elseif "SHB"==bank]
		[#return "shanghai.png"]
	[#elseif "SPDB"==bank]
		[#return "pudong.png"]
	[#elseif "CMBC"==bank]
		[#return "minsheng.png"]
	[#elseif "CMB"==bank]
		[#return "zhaoshang.png"]
	[#elseif "GDB"==bank]
		[#return "guangfa.png"]
	[/#if]
[/#function]